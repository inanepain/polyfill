{
    "name": "inanepain/polyfill",
    "description": "Various Polyfill functions for php backwards compatibility.",
    "type": "library",
    "license": "UNLICENSE",
    "homepage": "https://vaieen.local:3000/inane/polyfill",
    "readme": "README.adoc",
    "authors": [
        {
            "name": "Philip Michael Raab",
            "email": "peep@inane.co.za",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "peep@inane.co.za",
        "issues": "https://vaieen.local:3000/inane/polyfill/issues"
    },
    "keywords": [
        "inane",
        "polyfill",
        "shim",
        "compatibility",
        "portable"
    ],
    "autoload": {
        "psr-4": {
            "Inane\\Polyfill\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.0",
        "inanepain/stdlib": "*",
        "psr/log": "dev-master"
    },
    "require-dev": {}
}
